{
  "name": "servico-nomes_geograficos",
  "version": "1.0.0",
  "description": "Serviço de nomes geogra'ficos",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dsgoficial/servico_nomes_geograficos"
  },
  "engines": {
    "node": "~8.7.0",
    "npm": ">=5.4.2"
  },
  "scripts": {
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "config": "node create_config.js",
    "build": "node create_build.js",
    "create-docs": "node server/create_documentation.js",
    "start": "pm2 start server/src/index.js --name servico-edicao",
    "start-https": "pm2 start server/src/index.js --name servico-edicao-https -- --https ",
    "start-dev": "concurrently \"cd server && npm run dev\" \"cd client && npm run start\""
  },
  "keywords": [
    "Nomes geográficos",
    "Node",
    "Express"
  ],
  "author": "DSG/1CGEO <diniz.felipe@eb.mil.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsgoficial/servico_nomes_geograficos/issues"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg-promise": "^11.9.1"
  }
}
